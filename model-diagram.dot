digraph models_diagram {
	graph[overlap=false, splines=true]
	"Attachment" [shape=Mrecord, label="{Attachment|id :integer\lcreated_at :datetime\lupdated_at :datetime\lfile_file_name :string\lfile_content_type :string\lfile_file_size :integer\lfile_updated_at :datetime\ldraft_id :integer\l}"]
	"Draft" [shape=Mrecord, label="{Draft|id :integer\lcreated_at :datetime\lupdated_at :datetime\lsubject :string\lbody :text\lto :text\lcc :text\lbcc :text\luser_id :integer\l}"]
	"Token" [shape=Mrecord, label="{Token|id :integer\laccess_token :string\lrefresh_token :string\lexpires_at :datetime\lcreated_at :datetime\lupdated_at :datetime\luser_id :integer\l}"]
	"User" [shape=Mrecord, label="{User|id :integer\lemail :string\lencrypted_password :string\lreset_password_token :string\lreset_password_sent_at :datetime\lremember_created_at :datetime\lsign_in_count :integer\lcurrent_sign_in_at :datetime\llast_sign_in_at :datetime\lcurrent_sign_in_ip :string\llast_sign_in_ip :string\lcreated_at :datetime\lupdated_at :datetime\lname :string\lrole :string\l}"]
	"Draft" -> "Attachment" [arrowtail=odot, arrowhead=crow, dir=both color="#9F96B8"]
	"User" -> "Token" [arrowtail=odot, arrowhead=dot, dir=both color="#E1DFA4"]
	"User" -> "Draft" [arrowtail=odot, arrowhead=dot, dir=both color="#209D09"]
	"User" -> "Attachment" [arrowtail=crow, arrowhead=crow, dir=both color="#BCC361"]
}
